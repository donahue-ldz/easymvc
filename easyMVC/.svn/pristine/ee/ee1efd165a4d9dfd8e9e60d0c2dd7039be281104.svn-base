package com.easymvc.converter;

import com.easymvc.util.StringUtil;

/**
 * StringConverter
 * @author 唐延波
 * @date 2015-5-19
 */
public class StringConverter implements Converter {

	@Override
	public Object convert(String param) {
		if(StringUtil.isEmpty(param)){
			return null;
		}
		return param;
	}

}
